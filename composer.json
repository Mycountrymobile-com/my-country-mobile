{
  "name": "my-country-mobile/my-country-mobile",
  "description": "A Laravel Notification Channel for sending messages via Textlocal.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "My Country Mobile",
      "email": "support@mycountrymobile.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/notifications": "^7.0|^8.0|^9.0|^10.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "MyCountryMobile\\TextlocalChannel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MyCountryMobile\\TextlocalChannel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MyCountryMobile\\TextlocalChannel\\TextlocalServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
